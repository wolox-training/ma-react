{
  "name": "react-first-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "wolox-react-scripts": "1.2.1"
  },
  "scripts": {
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
